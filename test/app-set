apiVersion: argoproj.io/v1alpha1
kind: ApplicationSet
metadata:
  name: test-32
  namespace: argocd-system
spec:
  generators:
  - list:
      elements:
      - appname: sample-4323
      - appname: test-32
  template:
    metadata:
      name: 'bgd-tad-32'
    spec:
      project: default
      syncPolicy:
        automated:
          prune: true
          selfHeal: true
      source:
        repoURL: https://github.com/Pavan-Gunda/appset-demo.git
        targetRevision: master
        path: /test
      destination:
        server: https://kubernetes.default.svc
        namespace: production